<h1>Listing plot_points</h1>

<table>
  <tr>
    <th>Project</th>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @plot_points.each do |plot_point| %>
  <tr>
    <td><%= plot_point.project_id %></td>
    <td><%= plot_point.title %></td>
    <td><%= link_to 'Show', plot_point %></td>
    <td><%= link_to 'Edit', edit_plot_point_path(plot_point) %></td>
    <td><%= link_to 'Destroy', plot_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Plot point', new_plot_point_path %>
